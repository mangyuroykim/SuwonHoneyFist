package org.benq.day01.basic;

public class Exam_DataType {
	public static void main(String [] args) {
// System.out.println("출력하는 코드");
//		기본형
		/*
		 * 기본형
		 * 정수, 실수, 문자, 불린형
		 * 정수 4가지(byte, short, int, long)
		 * 실수 2가지(float, double)
		 * 문자 char
		 * 불린형 boolean
		 * 
		 * 참조형
		 * 문자열 String
		 */
		// 변수 선언 및 초기화(할당)
		boolean result = true;
		char fChar = 'a';
		System.out.println(result);
	}
}
