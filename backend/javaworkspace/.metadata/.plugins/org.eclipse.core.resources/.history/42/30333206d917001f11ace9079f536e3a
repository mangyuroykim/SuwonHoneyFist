package org.SuwonHoneyFist.day04.control.loop.exercise;

public class Exercise_forDouble2 {
    public static void main(String[] args) {
        // 0시 0분부터 23시 59분까지 출력하기

        for (int m = 0; m < 60; m++) { // 분 반복
            if (m == 0) { // 분이 0일 때만 시간 출력
                for (int h = 0; h < 24; h++) { // 시간 반복
                    System.out.println(h + "시 0분");
                }
            }
        }
    }
}
