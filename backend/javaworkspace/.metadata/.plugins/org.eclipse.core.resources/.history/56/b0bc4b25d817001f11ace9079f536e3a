package org.SuwonHoneyFist.day04.control.loop.exercise;

public class Exercise_forDouble2 {
	public static void main(String[] args) {
		// 0시 0분부터 23시 59분까지 출력하기

//		
		for (int m = 0; m < 60; m++) {
			for (int h = 0; h < 24; h++) {
				System.out.print(h + "시 ");
			}
			System.out.print(m + "분");
		}

	}
}
