			System.out.print("그 사이의 숫자들 : ");
			
		}
		if( num1 <= 1 && num2 <=1) {
			System.out.print(
					"그 사이의 숫자들 : ");
			for(int i=sum; )
		}else {
			System.out.print("1 이상의 숫자를 입력해주세요");
		}
	}
}
